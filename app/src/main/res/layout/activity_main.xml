<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.imist.realmdemo.MainActivity">
    <Button
        android:id="@+id/add1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ADD1"/>
    <Button
        android:id="@+id/add2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ADD2"/>
    <Button
        android:id="@+id/add3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ADD3"/>
    <Button
        android:id="@+id/addAsync"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="addAsync"/>
    <Button
        android:id="@+id/find1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="find1"/>
    <Button
        android:id="@+id/find2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="find2"/>
    <Button
        android:id="@+id/update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="update"/>
    <Button
        android:id="@+id/delete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="delete"/>

</LinearLayout>
